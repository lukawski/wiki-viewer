{
    "manifest_version": 2,

    "name": "Wikipedia Viewer Plugin",
    "description": "This extension is litttt",
    "version": "1.0",

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "index.html"
    },

    "permissions": [
        "https://en.wikipedia.org/w/api.php"
    ]
}